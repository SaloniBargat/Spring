<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xmlns:context = "http://www.springframework.org/schema/context"
   xmlns:p = "http://www.springframework.org/schema/p"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context.xsd">

   <bean  class = "com.springcore.dependencyinjection.Student" id="student1">
      <property name = "id" value = "10"/>
      <property name = "name" value = "Saloni"/>
      <property name = "address" value = "Airoli"/>
   </bean>
   
   <bean  class = "com.springcore.dependencyinjection.Student" id="student2">
      <constructor-arg name = "id" value="11"/>
      <constructor-arg name = "name" value="Sandeep"/>
      <constructor-arg name = "address" value="Mumbai"/>
   </bean>

</beans>
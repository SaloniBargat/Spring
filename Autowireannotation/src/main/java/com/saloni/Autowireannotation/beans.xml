<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xmlns:context = "http://www.springframework.org/schema/context"
   xmlns:p = "http://www.springframework.org/schema/p"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context.xsd">
   
   <context:annotation-config />
   
   <bean class="com.saloni.Autowireannotation.Course" name="course1">
   
   <property name="id" value="10" />
   <property name="course" value="java" />
   
   </bean>
   
    <bean class="com.saloni.Autowireannotation.Course" name="course2">
   
   <property name="id" value="10" />
   <property name="course" value="python" />
   
   </bean>
   
   <bean class="com.saloni.Autowireannotation.Student"  name="student1">
   
   <property name="name" value="Saloni"/>
   
   </bean>
  
   
 
   </beans>